type TeamId = text;
type Team = 
 record {
   donatedJerseys: nat;
   name: text;
   rewardPoints: nat;
   upcycledJerseys: nat;
 };
service : {
  getAllTeams: () -> (vec record {
                            TeamId;
                            Team;
                          }) query;
  getTeam: (TeamId) -> (opt Team) query;
  getTopDonatingTeams: (nat) -> (vec record {
                                       TeamId;
                                       Team;
                                     }) query;
  getTopRewardedTeams: (nat) -> (vec record {
                                       TeamId;
                                       Team;
                                     }) query;
  getTopUpcyclingTeams: (nat) -> (vec record {
                                        TeamId;
                                        Team;
                                      }) query;
  logDonation: (TeamId, nat) -> ();
  logUpcycled: (TeamId, nat) -> ();
  logWearingUsedJersey: (TeamId, nat) -> ();
  registerTeam: (TeamId, text) -> ();
}
